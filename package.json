{
    "name": "gulp-style-inject",
    "version": "0.0.0",
    "description": "A plugin for Gulp. Inject css file into html style tags.",
    "keywords": [
        "gulpplugin"
    ],
    "author": {
        "name": "Vlad Filip",
        "email": "vlad.filip.dev@gmail.ro",
        "url": "https://github.com/vladfilipro"
    },
    "repository": "vladfilipro/gulp-style-inject",
    "scripts": {
        "test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
        "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
    },
    "dependencies": {
        "through2": "*",
        "gulp-util": "~2.2.0"
    },
    "devDependencies": {
        "mocha": "*",
        "coveralls": "*",
        "mocha-lcov-reporter": "*",
        "istanbul": "*",
        "event-stream": "*",
        "should": "~2.1.0"
    },
    "engines": {
        "node": ">=0.8.0",
        "npm": ">=1.2.10"
    },
    "licenses": [ {
        "type": "MIT",
        "url": "http://github.com/vladfilipro/gulp-style-inject/raw/master/LICENSE"
    } ]
}